<Page x:Class="MusicApp.AddPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MusicApp"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AddPage">

    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Label Content="Jmeno kapely" Background="{x:Null}"></Label>
            <TextBox VerticalAlignment="Center" HorizontalAlignment="Center" Width="100" Margin="10"></TextBox>
            <Label Content="Rok založení" Background="{x:Null}"></Label>
            <TextBox VerticalAlignment="Center" HorizontalAlignment="Center" Width="100" Margin="10"></TextBox>
            
        </StackPanel>
    </Grid>
</Page>
